{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConsulConfig": {
    "ConsulClientAddress": "http://localhost:8500"
  },
  "harmony-scope": {
    "Person": {
      "Tag": "person-v1",
      "Controller": "api/person",
      "ById": "ById/{0}",
      "Paged": "Paged/{0}/{1}"
    },
    "City": {
      "Tag": "city-v1",
      "Controller": "api/city",
      "ById": "ById/{0}",
      "Paged": "Paged/{0}/{1}"
    }
  }
}
